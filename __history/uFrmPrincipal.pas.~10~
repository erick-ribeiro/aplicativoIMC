unit uFrmPrincipal;

interface

uses
  Winapi.Windows, Winapi.Messages, System.SysUtils, System.Variants, System.Classes, Vcl.Graphics,
  Vcl.Controls, Vcl.Forms, Vcl.Dialogs, Vcl.StdCtrls, Vcl.ExtCtrls, Vcl.Buttons, uFrmHelp;

type
  TfrmPrincipal = class(TForm)
    pnlTitle: TPanel;
    lblTitle: TLabel;
    lblHeight: TLabel;
    lblWeight: TLabel;
    lblGender: TLabel;
    pnlResult: TPanel;
    edtHeight: TEdit;
    edtWeight: TEdit;
    cmbGender: TComboBox;
    btnClear: TButton;
    btnResult: TButton;
    btnHelp: TSpeedButton;
    procedure btnHelpClick(Sender: TObject);
  private
    { Private declarations }
  public
    { Public declarations }
  end;

var
  frmPrincipal: TfrmPrincipal;

implementation

{$R *.dfm}

procedure TfrmPrincipal.btnHelpClick(Sender: TObject);
begin
  frmHelp.Show;
end;


end.
